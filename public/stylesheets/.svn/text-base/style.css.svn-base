body, div, ul, li, h1, h2, h3, h4, h5, h6, a, p, b, dl, dt, dd, span, img, i, em, input {
    margin: 0;
    padding: 0;
}

ul, ol, li {
    list-style: none;
}

img {
    border: none;
}

h1, h2, h3, h4, h5, h6, i, em {
    font-style: normal;
}

.cf:before, .cf:after {
    content: "";
    display: table;
}

.cf:after {
    clear: both;
}

.cf {
    zoom: 1;
}

a {
    color: #1a96d8;
    text-decoration: none;
}

a:hover {
    color: #1a96d8;
    text-decoration: underline;
}

.cut {
    overflow: hidden;
    text-overflow: ellipsis;
}

body {
    background-color: #c4f2ff;
    font: 400 12px/1.5 Arial, Helvetica, sans-serif;
    color: #333333;
}

/*layout*/
#layout {
    width: 100%;
    height: 0;
    background-color: #000000;
    -moz-opacity: 0.2;
    opacity: 0.2;
    filter: alpha(opacity = 20);
    z-index: 999;
    position: absolute;
    left: 0;
    top: 0;

}

.layout-wait {
    margin: 0 auto;
    z-index: 1000;
    position: absolute;
    left: 0;
    top: 0; /*-moz-opacity: 100;*/



/*opacity: 100;*/
/*filter:alpha(opacity=100);*/
    display: none;
}

/*messages box*/

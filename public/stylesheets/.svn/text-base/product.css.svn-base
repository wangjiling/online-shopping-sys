.user-info{
    border: 1px solid #c7c7c7;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: white;
    padding: 10px;
}
.user-info .user-detail{
    padding-bottom: 10px;
}
.user-info .user-detail table{
    width: 100%;
}
.user-info .user-history{
    border-top: 1px solid #cacaca;
    font-weight: 700;
    padding: 10px 20px 0;
}
.product .title,.others-like .title,.customer .title{
    height: 50px;
    background: url(../images/search-t-bg.png) repeat-x scroll 0 0;
    -moz-border-radius: 10px 10px 0 0;
    -webkit-border-radius: 10px 10px 0 0;
    border-radius: 10px 10px 0 0;
    line-height: 50px;
    padding-left: 20px;
}
.product .detail,.others-like .article,.customer .review{
    border-bottom: 1px solid #efefef;
    border-left: 1px solid #efefef;
    border-right: 1px solid #efefef;
    -moz-border-radius: 0 0 10px 10px;
    -webkit-border-radius: 0 0 10px 10px;
    border-radius: 0 0 10px 10px;
    background-color: white;
}
.product .title a{
    color: #333333;
}
.product .title span{
    font-weight: 700;
}
.product .detail{
    padding: 10px;
}
.product .detail .information{
    float: left;
    width: 495px;
    margin: 0 260px 0 205px;
    padding-bottom:60px;
    min-height:600px;
    position:relative;
}
.product .detail .information h2{
    color: #0078bd;
    font-size: 18px;
}
.product .detail .information .price{
    padding: 20px 0;
}
.product .detail .information .price dl,.product .detail .information .price dl dt,.product .detail .information .price dl dd{
    float: left;
}
.product .detail .information .price dl{
    padding: 0 5px 0 15px;
}
.product .detail .information .price dl dt{
    font-size: 14px;
    line-height: 28px;
}
.product .detail .information .price dl dd{
    font-size: 18px;
    font-weight: 700;
    padding-left: 5px;
}
.product .detail .information .price dl dd span{
    color: red;
}
.product .detail .information .description,.product .detail .information .restrictions{
    max-height:230px;
    padding:5px 0;
}
.product .detail .information .description p{
    padding-bottom: 20px;
}
.product .detail .information .restrictions p{
    color: #9a9999;
}
.product .detail .information .restrictions p b{
    display: block;
}
.product .detail .information .share{
    position:absolute;
    bottom:10px;
    left:0;
}
.product .detail .information .share>div{
    float: left;
    margin: 0 5px;
}
.product .detail .pic{
    float: left;
    width: 200px;
    margin-left: -960px;
}
.product .detail .pic div{
    padding: 10px 0;
}
.product .detail .pic img{
    width: 200px;
}
.product .detail .pic .store-logo,.product .detail .pic .store-logo img{
    height:100px;
}
.product .detail .pic .product-picture,.product .detail .pic .product-picture img{
    height:200px;
}
.product .detail .action{
    float: left;
    width: 255px;
    margin-left: -257px;
}
.product .detail .action>div{
    background: #fefefe url(../images/product-right-bg.png) repeat-x scroll 0 0;
    width: 215px;
    border: 1px solid #65caff;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    padding: 10px;
    margin: 10px auto;
}
.product .detail .action .buy-point h4{
    font-size: 14px;
}
.product .detail .action .buy-point .counts{
    padding: 10px 0;
    text-align: center;
}
.product .detail .action .buy-point .counts .add-count a{
    background-color: white;
    border: 1px solid #DBDBDB;
    color: #555555;
    cursor: pointer;
    display: inline-block;
    font-size: 12px;
    font-weight: 400;
    height: 9px;
    overflow: hidden;
    text-align: center;
    width: 9px;
    cursor: pointer;
}
.product .detail .action .buy-point .counts .add-count a:hover{
    border: 1px solid #ff0000;
    text-decoration: none;
}
.product .detail .action .buy-point .counts .add-count .minus{
    line-height: 7px;
}
.product .detail .action .buy-point .counts .add-count .plus{
    line-height: 9px;
}
.product .detail .action .buy-point .counts .add-count input[type="text"]{
    width: 25px;
    height: 18px;
    line-height: 18px;
    text-align: center;
    margin: 0 10px;
    border: 1px solid #ababab;
}
.product .detail .action .buy-point .counts .now{
    padding: 10px 0 0 26px;
}
.product .detail .action .buy-point .counts .now a{
    display: block;
    width: 169px;
    height: 51px;
    text-indent: -999em;
    overflow:hidden;
    cursor: pointer;
}
.product .detail .action .buy-point .counts .now .add{
    background: url(../images/purchase-bt-bg.png) no-repeat scroll 0 -148px;
}
.product .detail .action .buy-point .counts .now .buy-now{
    background: url(../images/purchase-bt-bg.png) no-repeat scroll 0 -94px;
}
.product .detail .action .buy-point .time-left .p1{
    font-size: 18px;
    text-align: center;
}
.product .detail .action .buy-point .deal-status{
    margin: 10px 0;
    border-top: 1px dashed #333333;
    text-align: center;
}
.product .detail .action .buy-point .deal-status .p1{
    font-size: 18px;
    font-weight: 700;
}
.product .detail .action .buy-point .deal-status .p2{
    font-weight: 700;
}
.product .detail .action .buy-point .deal-status .p3{
    background: url("../images/check-mark.png") no-repeat scroll 20px 4px transparent;
    font-size: 16px;
    font-weight: 700;
    line-height: 36px;
    padding-left: 10px;
}
.product .detail .action .buy-point .points{
    padding: 10px 0;
    margin: 10px 0;
    border-top: 1px dashed #333333;
    border-bottom: 1px dashed  #333333;
    font-size: 14px;
}
.product .detail .action .buy-point .points a{
    font-weight: 700;
    text-decoration: underline;
}
.product .detail .action .buy-point .benefits dt{
    text-decoration: underline;
}
.product .detail .action .calculator h4{
    font-size: 14px;
    padding-left: 35px;
    line-height: 30px;
    background: url(../images/calculator.png) no-repeat scroll 0 0;
}
.product .detail .action .calculator .amount>p{
    font-size: 14px;
    line-height: 3;
}
.product .detail .action .calculator .amount .amounter{
    padding: 10px 0 35px;
}
.product .detail .action .calculator .amount .amounter span{
    font-size: 14px;
    font-weight: 700;
    float: left;
    width: 50px;
    display: block;
}
.product .detail .action .calculator .amount .amounter .c2{
    position: relative;
    width: 177px;
    height: 12px;
    background: url(../images/calculator2.png) no-repeat scroll 0 0;
    float: left;
}
.product .detail .action .calculator .amount .amounter .c1{
    position: absolute;
    width: 21px;
    height: 36px;
    top:-12px;
    left:20px;
    background: url(../images/calculator1.png) no-repeat scroll 0 0;
    cursor: pointer;
}
.product .detail .action .calculator .amount .resoult p{
    font-size: 14px;
}
.product .detail .action .calculator .amount .resoult .p2{
    text-align: right;
}
.product .detail .action .calculator .amount .resoult .p3{
    text-align: right;
    color: #ff0000;
}
.product .detail .action .calculator .amount .resoult .p3 span{
    float: left;
}
.product .detail .action .calculator .amount .resoult .p4{
    text-align: center;
    border-top: 1px solid #333333;
}
.others-like .title h3{
    font-size: 18px;
}
.others-like .article{
    height: 300px;
    overflow: hidden;
}
.others-like .article .left,.others-like .article .deals,.others-like .article .right{
    float: left;
}
.others-like .article .left a,.others-like .article .right a{
    display: block;
    width: 33px;
    height: 300px;
    background: url(../images/left-right.png) no-repeat scroll 0 140px;
    margin: 0 3px;
    text-indent: -999em;
    cursor: pointer;
}
.others-like .article .right a{
    background-position: -33px 140px;
}
.others-like .article .deals{
    width: 900px;
    font-weight: 700;
    overflow: hidden;
}
.others-like .article .deals ul li{
    float: left;
    margin: 10px 5px;
    width: 170px;
}
.others-like .article .deals ul li .pic{
    background: url("../images/img-bg2.png") no-repeat scroll 0 0 transparent;
    height: 155px;
    overflow: hidden;
    padding: 10px;
    width: 155px;
}
.others-like .article .deals ul li .pic img{
    width: 150px;
    height: 150px;
}
.others-like .article .deals ul li .text{
    font-size: 14px;
}
.others-like .article .deals ul li .text a{
    color: #333333;
    display: block;
    height: 56px;
    line-height: 28px;
    text-align: center;
    overflow: hidden;
    text-overflow:ellipsis;
}
.others-like .article .deals ul li .text span{
    color: red;
    display: block;
    text-align: center;
}
.others-like .article .deals ul li .purchase{
    padding: 0 20px;
    text-align: center;
}
.others-like .article .deals ul li .purchase a{
    display: block;
    width: 126px;
    height: 30px;
    text-indent: -999em;
    background: url(../images/purchase-bt-bg.png) no-repeat scroll 0 0;
    overflow: hidden;
}
.customer .title h3{
    float: left;
    font-size: 18px;
    padding-right: 20px;
}
.customer .title span{
    float: left;
    font-weight: 700;
}
.customer .title span a{
    color: #333333;
}
.customer .title span a:hover{
    color: #1A96D8;
}
.customer .review .review-cont{
    margin: 10px 10px 0;
    border: 1px solid #cacaca;
    padding: 10px;
}
.customer .review .review-cont .photo{
    float: left;
    width: 90px;
    min-height: 90px;
}
.customer .review .review-cont .photo img{
    max-height: 90px;
}
.customer .review .review-cont .cont{
    float: right;
    width: 830px;
    padding-right: 10px;
}
.customer .review .review-cont .cont .theme{
    font-size: 14px;
    font-weight: 700;
}
.customer .review .review-cont .cont .time{
    color: #757575;
    padding: 10px 0;
}
.customer .review .review-cont .author{
    padding: 10px;
}
.customer .review .review-cont .author p span{
    display: inline-block;
    width: 85px;
}

.points_amount_slider {
    background: url("../images/calculator2.png") no-repeat scroll 0 0 transparent;
    position: relative;
    width: 160px;
    height: 12px;
    box-shadow: 1px 1px 1px 1px;
    float: left;
}


.points_amount_slider a, .silder_point{
    position: absolute;
    width: 21px;
    height: 36px;
    top:-12px;
    left:20px;
    background: url(../images/calculator1.png) no-repeat scroll 0 0;
    cursor: pointer;
}

.points_amount_slider a:hover{
    position: absolute;
    width: 21px;
    height: 36px;
    top:-12px;
    left:20px;
    background: url(../images/calculator1.png) no-repeat scroll 0 0;
    cursor: pointer;
}



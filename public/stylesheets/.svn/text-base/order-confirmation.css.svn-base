/*order_confirmation---S*/
.container .section .step-thr .step1, .container .section .step-thr .step2, .container .section .step-thr .step3 {
    width: 326px;
    text-align: center;
    font-size: 14px;
    font-weight: 700;
    color: #fff;
    float: left;
}

.container .section .step-thr .onhover {
    color: #333333;
}

.container .section .step-thr {
    width: 980px;
    height: 32px;
    line-height: 32px;
    background: url(../images/c-step2.png) no-repeat scroll 0 0px;
    float: left;
}

.container .section .step-thr5 {
    width: 980px;
    height: 30px;
    line-height: 30px;
    background: url(../images/c-step5.png) no-repeat scroll 0 -67px;
    float: left;
}

.container .section .step-thr5 .steps {
    width: 196px;
    text-align: center;
    font-size: 14px;
    font-weight: 700;
    color: #ffffff;
    float: left;
}

.container .section .step-thr5 .onhover {
    color: #333333;
}

.container .section .step-thr3 {
    background-position: 0 -32px;
}

.container .section .s-s {
    border: 1px solid #c7c7c7;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background-color: white;
    position: relative;
}

.container .section .s-s h3 {
    margin: 30px 0 0 60px;
    height: 67px;
    padding-left: 84px;
    font-size: 18px;
    font-weight: 700;
    color: #156590;
    line-height: 67px;
    background: url(../images/s-s-dr.png) no-repeat 0 center;
}

.container .section .s-py {
    margin-left: 144px;
    padding: 0 10px;
    width: 400px;
    border: 1px solid #d0c993;
    background: #ffffff;
}

.container .section .s-py p {
    font-size: 14px;
    line-height: 28px;
}


.container .section .s-py span {
    color: #c20202;
    font-weight: 700;
    margin-left: 10px;
}

.container .section .s-py2 {
    margin-left: 144px;
    padding: 20px;
    width: 380px;
    font-size: 16px;
    font-weight: 700;
    border: 1px solid #d0c993;
    background: #ffffff;
    text-align: right;
}

.container .section .s-py2 span {
    color: #ff0000;
}

.container .section .s-now {
    margin: 30px 0 30px 60px;
    color: #333333;
}

.container .section .s-now h4 {
    font-size: 14px;
    font-weight: 700;
}

.container .section .s-now p {
    padding-left: 65px;
    line-height: 48px;
}

.container .section .s-now p a {
    color: #333333;
    font-weight: 700;
    margin: 0 10px;
    text-decoration: none;
}

.container .section .s-now p a:hover {
    color: #ff0000;
    text-decoration: none;
}

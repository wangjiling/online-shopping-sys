.container .section .log .log-r .log-f div .bt {
    width: 62px;
    height: 27px;
    border: 0;
    background: url(../images/buttons.png) no-repeat scroll -144px -80px;
    text-indent:-99em;
    display: block;
    overflow: hidden;
    cursor: pointer;
}

.container .section .log .log-r .log-f div .ck {
    margin: 8px 0 0 5px;
}

.container .section .log .log-r .log-f span {
    position: absolute;
    top: 78px;
    left: 290px;
    font-weight:700;
}



/*login--S*/
.container .section .log {
    margin: 70px 0;
}

.container .section .log .log-l {
    float: left;
    width: 480px;
}

.container .section .log .log-r {
    float: right;
    width: 500px;
}

.container .section .log .log-r .log-f {
    margin: 10px 50px;
    padding:10px;
    border:1px solid #c7c7c7;
    -moz-border-radius:10px;
    -webkit-border-radius:10px;
    border-radius:10px;
    background-color:white;
    position:relative;
}


.container .section .log .log-r .log-f h2 {
    color: #4b7a07;
    line-height: 32px;
}

.container .section .log .log-r .log-f div p {
    font-size: 12px;
    color: #868282;
    font-weight: 700;
    line-height: 35px;
}

.container .section .log .log-r .log-f div .txt {
    width: 215px;
    height: 20px;
    border: 1px solid #939494;
}

